{
  "name": "Astro Index Page Pagination Demo",
  "description": "A repo to demonstrate paginating the index page",
  "version": "0.0.1",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "dev:h": "export NODE_ENV=development && astro dev --host",
    "start": "npm run dev",
    "start:h": "npm run dev:h",
    "build": "export NODE_ENV=production && astro build",
    "preview": "astro preview"
  },
  "keywords": [
    "astro",
    "blog"
  ],
  "dependencies": {
    "@astrojs/rss": "^1.0.0",
    "@astrojs/sitemap": "^1.0.0",
    "astro": "^1.1.2",
    "astro-icon": "^0.7.3",
    "config": "^3.3.7",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.6",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "astro-eslint-parser": "^0.5.1",
    "eslint": "^8.23.0",
    "eslint-plugin-astro": "^0.17.1",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.3"
  }
}
