{
  "title": "Astro Blogger",
  "description": "An Astro theme for blogging with index page pagination and full RSS.",
  "url": "http://localhost:3000/",
  "posts": {
    "path": "/",
    "paginationSize": 7,
    "heroWidths": [200, 400, 700, 1000],
    "heroSizes": "(max-width: 600px) 50vw, 75vw"
  },
  "rss": {
    "path": "/",
    "fileName": "rss.xml"
  }
}
